.ctaSection
  background: white
  border-radius: 16px
  padding: 3rem 2rem
  box-shadow: 0 15px 35px rgba(0, 0, 0, 0.25)
  margin-bottom: 4rem
  overflow: hidden
  position: relative
  
  &::before
    content: ''
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 5px
    background: linear-gradient(90deg, $blue1, $orange1)
  
  .ctaContent
    h2
      font-size: 2rem
      color: #222
      margin-bottom: 1rem
      text-shadow: 0 4px 10px rgba(0, 0, 0, 0.25)
      
      +xs
        font-size: 1.5rem
    
    p
      font-size: 1.1rem
      color: #444
      margin-bottom: 2rem
      text-shadow: 0 3px 6px rgba(0, 0, 0, 0.2)
      
      +xs
        font-size: 1rem
    
    .ctaButtons
      display: flex
      gap: 1rem
      flex-wrap: wrap

  .ctaImage
    display: flex
    align-items: center
    justify-content: center
    
    .notificationPreview
      width: 100%
      max-width: 350px
      background: white
      border-radius: 12px
      overflow: hidden
      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.15)
      border: 1px solid #eee
      animation: pulse 2s infinite
      
      .notificationHeader
        background: $blue1
        color: white
        padding: 12px 16px
        display: flex
        align-items: center
        gap: 8px
        font-weight: 600
      
      .notificationContent
        padding: 16px
        
        p
          font-size: 1rem
          color: #222
          margin-bottom: 8px
        
        .notificationTime
          font-size: 0.8rem
          color: #666

