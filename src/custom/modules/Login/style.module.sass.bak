// Add styles for modal backgrounds with wave effects
:global
  .ant-modal-content
    background: linear-gradient(-45deg, rgba(24, 142, 255, 0.1), rgba(255, 127, 80, 0.1))
    backdrop-filter: blur(10px)
    border-radius: 16px
    overflow: hidden
    position: relative
    box-shadow: 0 20px 50px rgba(0, 0, 0, 0.3)
    
    &::before
      content: ''
      position: absolute
      top: 0
      left: 0
      right: 0
      height: 4px
      background: linear-gradient(90deg, rgba(24, 142, 255, 1), rgba(255, 127, 80, 1))
      z-index: 3
    
    .modal-wave-container
      position: absolute
      width: 100%
      height: 100%
      top: 0
      left: 0
      z-index: 0
      overflow: hidden
    
    .modal-wave
      position: absolute
      width: 200%
      height: 60px
      left: 0
      background: transparent
      opacity: 0.2
      border-radius: 0
      filter: blur(8px)
      
    .modal-wave1
      bottom: 0
      height: 40px
      background: rgba(24, 142, 255, 0.6)
      animation: waveAnimation 15s linear infinite
      
    .modal-wave2
      bottom: 20px
      height: 35px
      background: rgba(255, 127, 80, 0.5)
      animation: waveAnimationReverse 12s linear infinite
    
    .ant-modal-header
      background: transparent
      border-bottom: 1px solid rgba(255, 255, 255, 0.2)
      
      .ant-modal-title
        color: rgba(0, 0, 0, 0.85)
        font-weight: 600
        text-shadow: 0 2px 4px rgba(0, 0, 0, 0.15)
    
    .ant-modal-body
      position: relative
      z-index: 1
      
    .ant-modal-footer
      background: transparent
      border-top: 1px solid rgba(255, 255, 255, 0.2)
      position: relative
      z-index: 1

// Coin Swap Modal Styling
.coinSwapModal
  :global
    .ant-modal-content
      background: linear-gradient(-45deg, rgba(24, 142, 255, 0.15), rgba(255, 127, 80, 0.15))
      
      .ant-table
        background: rgba(255, 255, 255, 0.85)
        backdrop-filter: blur(5px)
        border-radius: 12px
        overflow: hidden
        box-shadow: 0 10px 25px rgba(0, 0, 0, 0.15)
        
      .ant-form-item
        position: relative
        z-index: 1
      
      .ant-input, .ant-select-selector, .ant-btn
        backdrop-filter: blur(5px)
        position: relative
        z-index: 1
      
      .modal-orb1
        position: absolute
        width: 150px
        height: 150px
        top: 10%
        right: -5%
        background: radial-gradient(circle, rgba(24, 142, 255, 0.3), rgba(24, 142, 255, 0) 70%)
        border-radius: 50%
        filter: blur(20px)
        animation: float 10s ease-in-out infinite alternate
        z-index: 0
      
      .modal-orb2
        position: absolute
        width: 120px
        height: 120px
        bottom: 10%
        left: -5%
        background: radial-gradient(circle, rgba(255, 127, 80, 0.3), rgba(255, 127, 80, 0) 70%)
        border-radius: 50%
        filter: blur(20px)
        animation: float 8s ease-in-out infinite alternate-reverse
        z-index: 0

